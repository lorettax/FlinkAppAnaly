<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="OFF">

  <Appenders>
    <Console name="STDOUT">
      <PatternLayout pattern="%d %-5p [%t] %C{2} (%F:%L) - %m%n"/>
    </Console>
  </Appenders>
   
  <Loggers>
    <Root level="INFO">
      <AppenderRef ref="STDOUT"/>
    </Root>
    <Logger name="com.youfan.test" level="debug" additivity="false">
      <AppenderRef ref="STDOUT" />
    </Logger>
  </Loggers>

</Configuration>